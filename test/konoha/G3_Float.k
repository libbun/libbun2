


function TestFloatFloat(x: float, y: float) {
println("#x=1.0,+x: " + (+x));
println("#x=1.0,-x: " + (-x));
println("#x=1.0,y=1.1,x+y: " + (x+y));
println("#x=1.0,y=1.1,x-y: " + (x-y));
println("#x=1.0,y=1.1,x*y: " + (x*y));
println("#x=1.0,y=1.1,x/y: " + (x/y));
println("#x=1.0,y=1.1,x==y: " + (x==y));
println("#x=1.0,y=1.1,x!=y: " + (x!=y));
println("#x=1.0,y=1.1,x<y: " + (x<y));
println("#x=1.0,y=1.1,x<=y: " + (x<=y));
println("#x=1.0,y=1.1,x>y: " + (x>y));
println("#x=1.0,y=1.1,x>=y: " + (x>=y));
}


export function main() {
	println("#0.0: " + 0.0);
	println("#1.0: " + 1.0);
	TestFloatFloat(1.0, 1.1);
}
	
