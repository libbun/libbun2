function ReturnBoolean() : boolean {
	assert(true);
}

function ReturnInt() : int {
	assert(true);
}

function ReturnString() : String {
	assert(true);
}

function ReturnVoid() {
	assert(true);
}

export function main() {
	ReturnVoid();
	println("#return default boolean: "+ ReturnBoolean());
	println("#return default int: " + ReturnInt());
	//println("#return default String: " + ReturnString()); 
	assert(!ReturnBoolean());
	assert(ReturnInt() == 0);
	ReturnVoid();
}

